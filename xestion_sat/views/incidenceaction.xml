<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id='xestionsat_actuacionsincidencia_action' model='ir.actions.act_window'>
        <field name="name">Actuacions Incidencias</field>
        <field name="res_model">xestionsat.actuacionsincidencia</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree</field>
    </record>

    <!-- Tree(list) View -->
    <record id="xestionsat_actuacionsincidencia_view_tree" model="ir.ui.view">
        <field name="name">Lista Actuacions Incidencias</field>
        <field name="model">xestionsat.actuacionsincidencia</field>
        <field name="arch" type="xml">
            <tree>
                <field name="executado_por_id"/>
                <field name="template_id"/>
                <field name="observacions"/>
                <field name="list_price"/>
                <field name="data_ini"/>
                <field name="data_fin"/>
            </tree>
        </field>
    </record>
    
</odoo>
