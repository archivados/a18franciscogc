<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Tree(list) View -->
    <record id="xestionsat_device_component_view_tree" model="ir.ui.view">
        <field name="name">Device Components</field>
        <field name="model">xestionsat.device.component</field>
        <field name="arch" type="xml">
            <tree>
                <field name="template_id"/>
                <field name="serial"/>
                <field name="observation"/>
                <field name="date_registration"/>
                <field name="date_cancellation"/>
            </tree>
        </field>
    </record>
</odoo>
